{
  "name": "solana-telegram-mini-app",
  "version": "1.0.0",
  "description": "Solana Token Launcher Telegram Mini App",
  "main": "dist/server.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build:backend": "tsc --skipLibCheck --noImplicitAny false", 
    "start:backend": "node dist/server.js",
    "dev:backend": "nodemon --exec ts-node backend/server.ts",
    "start:frontend": "cd frontend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run start:frontend\"",
    "start": "node dist/server.js",
    "build": "npm run build:backend && npm run build:frontend",
    "heroku-postbuild": "npm run install:all && npm run build"
  },
  "keywords": [
    "raydium",
    "solana",
    "token",
    "pool",
    "liquidity",
    "telegram"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@raydium-io/raydium-sdk-v2": "latest",
    "@solana/spl-token": "^0.3.8",
    "@solana/web3.js": "^1.78.0",
    "@types/bn.js": "^5.1.6",
    "bn.js": "^5.2.1",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.6.0",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "process": "^0.11.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
